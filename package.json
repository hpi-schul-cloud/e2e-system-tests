{
	"name": "e2e-system-tests",
	"version": "1.0.0",
	"description": "e2e testing",
	"main": "index.js",
	"scripts": {
		"pretest": "if exist cypress\\reports; then rmdir /S/Q cypress\\reports; fi;",
		"generate:report": "node reporter.js",
		"cy:open:local": "cypress open --browser chrome -e tags=@stable_test,environmentName=local",
		"cy:open:staging": "cypress open --browser chrome -e tags=\"@stable_test and (not @non_staging_test)\",environmentName=staging",
		"tag:stable:staging": "cypress run --browser chrome -e tags=\"@stable_test and (not @non_staging_test)\",environmentName=staging",
		"tag:stable:local": "cypress run --browser chrome -e tags=\"@stable_test and @brb\",environmentName=local",
		"tag:stable:pr:local": "cypress run --browser chrome -e tags=\"@pr and @stable_test\",environmentName=local",
		"tag:stable:release:local": "cypress run --browser chrome -e tags=\"@release and @stable_test\",environmentName=local",
		"tag:only:local": "cypress run --browser chrome -e tags=\"@only and @stable_test and @brb\",environmentName=local",
		"tag:unstable:local": "cypress run --browser chrome -e tags=@unstable_test,environmentName=local",
		"cy:open:ci": "cypress open --browser chrome -e tags=@stable_test,environmentName=ci",
		"tag:stable:ci": "xvfb-run cypress run --browser chrome -e tags=\"@stable_test and @brb\",environmentName=ci",
		"tag:stable:pr:ci": "xvfb-run cypress run --browser chrome -e tags=\"@pr and @stable_test\",environmentName=ci",
		"tag:stable:release:ci": "xvfb-run cypress run --browser chrome -e tags=\"@release and @stable_test\",environmentName=ci",
		"tag:only:ci": "xvfb-run cypress run --browser chrome -e tags=@only,environmentName=ci",
		"tag:unstable:ci": "xvfb-run cypress run --browser chrome -e tags=@unstable_test,environmentName=ci"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hpi-schul-cloud/e2e-system-tests.git"
	},
	"keywords": [
		"e2e"
	],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/hpi-schul-cloud/e2e-system-tests/issues"
	},
	"homepage": "https://github.com/hpi-schul-cloud/e2e-system-tests#readme",
	"devDependencies": {
		"@badeball/cypress-cucumber-preprocessor": "^18.0.6",
		"@cypress/webpack-preprocessor": "^5.17.0",
		"cypress": "^13.2.0",
		"cypress-file-upload": "^5.0.8",
		"multiple-cucumber-html-reporter": "^1.21.4",
		"webpack": "^5.77.0"
	},
	"cypress-cucumber-preprocessor": {
		"stepDefinitions": "cypress/support/step_definition/**/*.js",
		"filterSpecs": true,
		"omitFiltered": true,
		"json": {
			"enabled": true,
			"output": "logs/json-report.json"
		},
		"messages": {
			"enabled": false,
			"output": "logs/messages.ndjson"
		},
		"html": {
			"enabled": false,
			"output": "html-report/report.html"
		}
	}
}
